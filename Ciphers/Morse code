letter_sep = ' ' #input("What is used to sep your letters? ")
word_sep = '/' #input("What is used to sep your words? ")

msg = '... .-. .. -. --. -... .- -.-. -.- - .... . - .-. . . ...'
#'..... ....- / ..... ---.. / -.... -... / -.... --... / -.... ..--- / ..... --... / ....- -.... / --... ....- / ..... ----. / ..... ...-- / ....- ..--- / -.... ---.. / -.... ..--- / ....- ---.. / -.... ....- / -.... ---.. / -.... ..... / ..... ---.. / ....- -.. / -.... --... / -.... ...-- / ...-- ..--- / ....- -.... / --... ----- / ..... .- / ....- ...-- / --... --... / -.... --... / -.... ..--- / ....- --... / -.... -.-. / -.... -.. / ..... .- / ..... ...-- / ....- ..--- / --... ----- / -.... ...-- / --... ----. / ....- ..--- / --... ...-- / -.... .---- / ..... --... / --... ....- / -.... -.-. / ....- ----. / ....- --... / ....- ..... / -.... --... / ..... ----. / -.... -.. / ...-- ----. / ...-- ....- / ....- ----. / ....- --... / ...-- ----. / -.... -.. / ....- ----. / ....- --... / ....- . / -.... ..-. / -.... ..--- / ...-- ..--- / ....- . / --... -.... / -.... ..--- / ....- --... / ....- -.... / ...-- ----- / ..... .- / ..... ---.. / ....- -.. / --... ..... / ....- ----. / ....- -.... / -.... -.-. / --... -.... / -.... ....- / ..... ...-- / ....- ..--- / --... ..... / ..... .- / ..... ---.. / ..... .- / -.... -.-. / -.... ...-- / -.... ----. / ....- ..--- / --... ..--- / -.... ..--- / -.... -.. / ...-- ----. / ...-- ...-- / ....- ----. / ....- ---.. / -.... ....- / -.... ..-. / ..... ----. / ..... ---.. / ..... .---- / -.... --... / -.... ..... / ..... --... / ...-- ----. / ...-- .---- / ....- .- / ...-- ...-- / ....- .- / -.... -.-. / ....- ----. / ....- --... / -.... ....- / --... -.... / -.... ..--- / -.... -.. / ...-- ..... / -.... ---.. / ....- ----. / ....- --... / -.... ....- / -.... -.-. / -.... ....- / ....- ...-- / ...-- ....- / ....- -... / ....- ...-- / -.... -... / -.... ---.. / -.... -.-. / -.... ...-- / -.... -.. / ..... ..... / -.... . / -.... ...-- / --... ----. / ....- ..--- / ...-- ..... / -.... ..--- / ...-- ...-- / ..... -.... / --... ----. / ....- ----. / ....- --... / ..... .- / --... ----- / -.... ...-- / -.... . / ....- . / ...-- ----- / ....- ----. / ....- --... / ..... .- / --... ...-- / ..... ----. / ..... --... / -.... ...-- / ...-- -.... / ....- ----. / ....- ..... / ...-- .---- / ..... ----- / ..... .---- / ...-- ----- / --... ....- / ...-- --... / ....- . / --... .- / ..... .---- / ...-- ...-- / ....- . / -.... .- / ....- ----. / ...-- .---- / ....- -.. / ..... ....- / -.... ...-- / --... ---.. / ..... .- / ..... ....- / ..... .---- / ...-- ...-- / ....- -.. / ..... --... / ....- ----. / --... .- / ..... ----. / ...-- ..--- / ....- ..... / ...-- ...-- / ....- -.. / ....- ....- / -.... --... / --... .- / ..... ----. / --... .- / -.... -.-. / -.... -.. / ..... .- / ..... --... / ..... .---- / --... --... / ..... ----. / --... .- / -.... --... / --... .- / ....- . / ..... ....- / -.... ...-- / --... ----. / ....- -.. / ....- ....- / -.... --... / ...-- .---- / ..... .- / -.... -.. / ....- -.. / --... --... / ....- -.. / ....- ....- / -.... ....- / -.... .- / ....- . / --... .- / ..... ..--- / -.... .- / ....- -.. / -.... .- / ..... .---- / ...-- ...-- / ....- . / ..... ....- / ..... ----. / ...-- .---- / ..... ----. / ..... ....- / ..... .- / -.... -... / ....- ..-. / ....- --... / ....- -.. / ...-- ...-- / ....- . / -.... .- / ....- -.... / -.... ----. / ..... .- / -.... .- / ....- -.... / -.... -.-. / ..... .- / -.... . / ...-- ----- / ...-- -..'
