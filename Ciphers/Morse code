#'.-- .... ....- --...//.... ....- --... ....//----. ----- -..//.-- ..--- ----- ..- ----. .... --...' #input("Msg: ")

words = msg.strip().split('//')
val = list(morse_code.values())
keys = list(morse_code.keys())

ctf = ''
for a in range(len(words)):
    word = words[a].strip().split(' ')
    for b in range(len(word)):
        for c in range(len(val)):
            if word[b] == val[c]:
                ctf += keys[c]
                break
    if a != len(words)-1:
        ctf += '_'

print(ctf.lower())
